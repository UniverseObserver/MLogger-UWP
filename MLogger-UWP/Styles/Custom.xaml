<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:controls="using:Template10.Controls">

    <x:Double x:Key="NarrowMinWidth">0</x:Double>
    <x:Double x:Key="NormalMinWidth">640</x:Double>
    <x:Double x:Key="WideMinWidth">1280</x:Double>

    <x:Double x:Key="NarrowMinHeight">100</x:Double>
    <x:Double x:Key="NormalMinHeight">300</x:Double>

    <Color x:Key="CustomColor">#FFFF4081</Color>
    <!--Main color and -->
    <Color x:Key="ContrastColor">White</Color>
    <!--Title bar text color-->
    <Color x:Key="SystemAccentColor">#FFF23D7A</Color>
    <!--unknown-->

</ResourceDictionary>

